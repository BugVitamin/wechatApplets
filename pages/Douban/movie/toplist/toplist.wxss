@import "../../pub/pub.wxss";

/* toplist.wxss */

page {
  width: 100%;
  height: 100%;
  background-color: #f4f9f9;
}
.heads{
  width:200rpx;
  height:60rpx;
  font-size:50rpx;
  position:relative;
  margin-left:275rpx;
  margin-top:50rpx;
  text-align:center;
}
.goback{
  width:130rpx;
  height:130rpx;
  position:fixed;
  z-index:999;
  top:15rpx;
  right:0px;
  left:-10rpx;
}
.tabbars {
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  border-bottom: 2px solid #999;
  box-shadow: 0 1rpx 1rpx #999;
  background-color: rgba(	18,166,183,.2);
}

.tabbarsitempa {
  width: 100%;
  height: 100%;
  font-size: 26rpx;
  color: #999;
  display: flex;
  margin: auto 20rpx;
}

.tabbarsitem {
  margin: auto;
  font-size:24rpx;
}

.list {
  margin: 40rpx 0px;
  padding: 0px;
  display: flex;
  flex-direction: column;
}
